	 IFTHEN B002=40
		 MACRO1 MJ#(3)
		 CALL JOB:GET_NEW_FLANGE_POS-1-STAT-1-H&D
		 MACRO1 MJ#(4)
	 ENDIF
	'***** flange check
	 IFTHEN B002=50
		 MACRO1 MJ#(3)
		 CALL JOB:CONTOUR_MEASUREMENT-PRODUCT-S1
		 MACRO1 MJ#(4)
	 ENDIF
	'***** return flange
	 IFTHEN B002=60
		 MACRO1 MJ#(3)
		 CALL JOB:RETURN_FLANGE_TO-POS-1
		 MACRO1 MJ#(4)
	 ENDIF
	'***** rotation measurement
	 IFTHEN B002=70
		 MACRO1 MJ#(3)
		 CALL JOB:ROTATION_MEASUREMENT
		 MACRO1 MJ#(4)
	 ENDIF
	'***** repick flange for orientat
	 IFTHEN B002=80
		 MACRO1 MJ#(3)
		 CALL JOB:REPICK_FLANGE_-POSITION-1-STAT-1
		 MACRO1 MJ#(4)
	 ENDIF
	'***** home position
	 IFTHEN B002=90
		 MACRO1 MJ#(3)
		 CALL JOB:HOME-POSITION-ROBOT
		 MACRO1 MJ#(4)
	 ENDIF
ENDIF
