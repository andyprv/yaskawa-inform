	 IFTHEN B002=30
		 MACRO1 MJ#(3)
		'get flange diameter
		 MACRO1 MJ#(1) ARGF1 ARGF10
		 PAUSE IF B000=1
		'get flange hight
		 MACRO1 MJ#(1) ARGF2 ARGF6
		 PAUSE IF B000=1
